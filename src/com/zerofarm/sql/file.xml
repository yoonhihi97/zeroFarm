<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="File">
	<insert id="insertFile" parameterType="filedto">
		INSERT INTO TBL_FILE VALUES(#{systemname},#{orgname},#{camnum})
	</insert>
	<select id="getFiles" parameterType="_int" resultType="filedto">
		SELECT * FROM TBL_FILE WHERE CAMNUM=#{camnum}
	</select>
	<delete id="deleteFileByName" parameterType="string">
		DELETE FROM TBL_FILE WHERE SYSTEMNAME=#{systemname}	
	</delete>
</mapper>












